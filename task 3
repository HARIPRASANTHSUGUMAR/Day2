public class main
    {
         public static void main (String[]args)
        {
            int a=5,b=6;
            if(a%2==0)
                System.out.println("it is even");
            else
                System.out.println("it is odd");
            if(b%2==0)
                 System.out.println("it is even");
            else
                 System.out.println("it is odd");
        }
    }
